<% provide(:title, 'Listings') %>
<h1> New Listings</h1>
<% if user_signed_in? %>
	<%= form_for(@listing) do |f| %>
		<%= f.label "Start" %>
		<%= f.text_field :datein, :class => 'datepicker'  %>

		<%= f.label "Finish" %>
		<%= f.text_field :dateout, :class => 'datepicker'  %>

		<%= f.submit "Create Listing" %>
	<% end %>
	<!-- put stuff here later -->
<% end %>